{
  "name": "nextjs-rest-mongodb",
  "version": "0.1.0",
  "main": "server.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "dev": "npx supervisor -n -t -w back-end,config,shared,next.config.js,server.js server.js",
    "lint": "eslint back-end config front-end shared --ext .js,.jsx",
    "mongo": "docker start nextjs-rest-mongodb || npm run mongoup",
    "mongoup": "docker run -d --rm -p27017:27017 -v /tmp/nextjs_dev_db/:/data/db/ --name nextjs-rest-mongodb mvertes/alpine-mongo; exit 0",
    "predev": "cp -n .env.example .env; npm run mongo",
    "prestart": "next build ./front-end",
    "start": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.3",
    "ajv": "^6.10.2",
    "clsx": "^1.0.4",
    "dotenv-safe": "8.1.0",
    "fastify": "2.7.1",
    "fastify-cookie": "^3.1.0",
    "fastify-static": "^2.5.0",
    "get-form-data": "^2.0.0",
    "isomorphic-unfetch": "3.0.0",
    "jss": "^9.8.7",
    "lodash": "4.17.15",
    "mongodb": "^3.3.2",
    "next": "9.1.1",
    "prop-types": "15.7.2",
    "query-string": "6.8.3",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.2",
    "faker": "^4.1.0",
    "prettier": "^1.18.2",
    "supervisor": "^0.12.0"
  },
  "description": "Starter kit for using nextjs, rest client and mongodb",
  "license": "MIT",
  "repository": "git@gitlab.com:plusbpm/nextjs-rest-mongodb.git"
}