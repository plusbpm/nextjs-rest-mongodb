{
  "name": "nextjs-rest-mongodb",
  "version": "0.1.0",
  "main": "server.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "build": "next build ./react",
    "backend:pro": "NODE_ENV=production node ./back-end/server.js",
    "backend:dev": "NODE_ENV=development npx supervisor -n -t -w back-end,.env,shared,next.config.js ./back-end/server.js",
    "dev": "run-p -l backend:dev react:dev",
    "docker_up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker_stop": "docker-compose -f docker/docker-compose.yml stop",
    "lint": "eslint back-end config react shared --ext .js,.jsx",
    "mongo": "docker start nextjs-rest-mongodb || npm run mongoup",
    "mongoup": "docker run -d --rm -p27017:27017 -v /tmp/nextjs_dev_db/:/data/db/ --name nextjs-rest-mongodb plsbpm/mongodb-replica:scratch; exit 0",
    "copy_env_example": "[ ! -e .env ] && cp .env.example .env",
    "predev": "npm run copy_env_example;  npm run mongo",
    "prestart": "npm run copy_env_example",
    "react:pro": "NODE_ENV=production node ./react/server.js",
    "react:dev": "NODE_ENV=development node ./react/server.js",
    "start": "run-p -l backend:pro react:pro"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "ajv": "^6.10.2",
    "big.js": "^5.2.2",
    "clsx": "^1.0.4",
    "cookie": "^0.4.0",
    "dotenv-safe": "8.1.0",
    "fastify": "2.10.0",
    "fastify-cookie": "^3.2.0",
    "fastify-http-proxy": "^2.3.0",
    "fastify-static": "^2.5.0",
    "get-form-data": "^2.0.0",
    "isomorphic-unfetch": "3.0.0",
    "jss": "^10.0.0",
    "lodash": "4.17.15",
    "mongodb": "^3.3.3",
    "next": "9.1.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "15.7.2",
    "query-string": "6.8.3",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-merge": "^4.2.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "faker": "^4.1.0",
    "prettier": "^1.18.2",
    "supervisor": "^0.12.0"
  },
  "description": "Starter kit for using nextjs, rest client and mongodb",
  "license": "MIT",
  "repository": "git@gitlab.com:plusbpm/nextjs-rest-mongodb.git"
}